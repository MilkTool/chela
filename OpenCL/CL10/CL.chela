        public static 
        int BuildProgram(IntPtr program, Int32 num_devices, IntPtr[] device_list, String options, IntPtr pfn_notify, IntPtr user_data)
        {
            unsafe
            {
                fixed (IntPtr* device_list_ptr = device_list)
                {
                    StringHandle options_ptr = StringHandle.Alloc(options);
                    try
                    {
                        return Delegates.clBuildProgram((IntPtr)program, (uint)num_devices, (IntPtr*)device_list_ptr, options_ptr.Address, (IntPtr)pfn_notify, (IntPtr)user_data);
                    }
                    finally
                    {
                        options_ptr.Free();
                    }
                }
            }
        }

        public static 
        int BuildProgram<T5>(IntPtr program, Int32 num_devices, IntPtr[] device_list, String options, IntPtr pfn_notify, T5[] user_data)
